<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <servlet>
        <servlet-name>ProcessResultsServlet</servlet-name>
        <servlet-class>VigulearLab3Servlets.ProcessResultsServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>ProcessResultsServlet</servlet-name>
        <url-pattern>/VigulearLab3Servlets.ProcessResultsServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>ServletToJSP</servlet-name>
        <servlet-class>VigulearLab3Servlets.ServletToJSP</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>ServletToJSP</servlet-name>
        <url-pattern>/VigulearLab3Servlets.ServletToJSP</url-pattern>
    </servlet-mapping>

</web-app>

